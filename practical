#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
	char ch[1000];
	int i;
	printf("Enter a word : \n");
 gets(ch);
	for(i=0;i<1000;i++)
	{
		char c=ch[i];
		if(c==NULL)
		break;
		else if(c==' ')
		printf("whitespace\n");
		else
		{
		switch(c)
		{
			case 'a':
				printf("    %c is a vowel\n",c);
				break;
				case 'e':
				printf("    %c is a vowel\n",c);
				break;
				case 'i':
				printf("    %c is a vowel\n",c);
				break;
				case 'o':
				printf("    %c is a vowel\n",c);
				break;
				case 'u':
				printf("    %c is a vowel\n",c);
				break;
				case 'A':
				printf("    %c is a vowel\n",c);
				break;
				case 'E':
				printf("    %c is a vowel\n",c);
				break;
				case 'I':
				printf("    %c is a vowel\n",c);
				break;
				case 'O':
				printf("    %c is a vowel\n",c);
				break;
				case 'U':
				printf("    %c is a vowel\n",c);
				break;
				default:
					if(c=='b'||c=='c'||c=='d'||c=='f'||c=='g'||c=='h'||c=='j'||c=='k'||c=='l'||c=='m'||c=='n'||c=='p'||c=='q'||c=='r'||c=='s'||c=='t'||c=='v'||c=='w'||c=='x'||c=='y'||c=='z')
					{
						printf("%c is a consonant\n",c);
					}
					else if(c=='B'||c=='C'||c=='D'||c=='F'||c=='G'||c=='H'||c=='J'||c=='K'||c=='L'||c=='M'||c=='N'||c=='P'||c=='Q'||c=='R'||c=='S'||c=='T'||c=='V'||c=='W'||c=='X'||c=='Y'||c=='Z')
					printf("%c still an UPPERCASE consonant\n",c);
					else
					printf("something else\n");
		}
	}
	}
	return 0;
}
